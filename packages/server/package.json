    "@types/express": "^5.0.1",
    "@types/jest": "29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
