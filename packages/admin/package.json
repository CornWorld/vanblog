  "name": "@vanblog/admin",
  "version": "0.54.0-corn.4",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "vite build --mode analyze",
    "build": "vite build",
    "deploy": "pnpm run build && pnpm run gh-pages",
    "dev": "vite",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=1000 --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=1000",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --max-warnings=1000 --format=pretty ./src && pnpm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --max-warnings=1000 --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\"",
    "playwright": "playwright install && playwright test",
    "prepare": "echo 'pre'",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "vite preview",
    "start": "vite --port 3002",
    "test": "vitest run",
    "test:component": "vitest run ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
